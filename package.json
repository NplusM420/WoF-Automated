{
  "name": "spotspinner-automation",
  "version": "1.0.0",
  "description": "Complete automation for Wheel of Fate MULTIBALL tokens on ApeChain",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node src/server.js",
    "dev-advanced": "node src/dev-server.js",
    "server": "node src/server.js",
    "ui": "cd ui && npm start",
    "build": "cd ui && npm run build",
    "dev-ui": "concurrently \"npm run server\" \"npm run ui\"",
    "web": "npm run build && npm run server",
    "install-ui": "cd ui && npm install",
    "secure-setup": "node src/secure-setup.js setup",
    "secure-test": "node src/secure-setup.js test",
    "secure-status": "node src/secure-setup.js status",
    "secure-delete": "node src/secure-setup.js delete",
    "security-audit": "node src/secure-setup.js status && find . -name '*.env*' -type f 2>/dev/null | head -5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "express": "^4.18.2",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "http-proxy-middleware": "^3.0.5",
    "nodemon": "^3.0.1"
  },
  "type": "module"
}
